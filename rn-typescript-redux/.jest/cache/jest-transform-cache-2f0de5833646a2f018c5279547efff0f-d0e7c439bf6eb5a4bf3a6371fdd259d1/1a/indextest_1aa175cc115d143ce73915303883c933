      var expectedActions = [{
        type: 'home/CHANGE_TEXT',
        text: ''
      }];
      var actions = [_index.actionCreators.changeText()];
      expect(actions).toEqual(expectedActions);
    });
  });
  describe('reduce', function () {
    var state = (0, _index.default)(undefined, {});
    it('should return the initialState', function () {
      expect(state).toEqual(_index.initialState);
    });
    it('change text to "hi"', function () {
      state = (0, _index.default)(state, _index.actionCreators.changeText('hi'));
      expect(state).toHaveProperty('text', 'hi');
    });
  });
});
